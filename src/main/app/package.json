{
  "name": "kouta-ui",
  "version": "0.1.0",
  "homepage": "/kouta",
  "private": true,
  "scripts": {
    "prepare": "cd ../../../ && husky",
    "start": "vite dev",
    "start:integration": "cross-env DISABLE_LOCAL_PROXY=true NODE_ENV=test pnpm run start",
    "start:integration:debug": "cross-env VITE_CIMODE=1 NODE_ENV=test pnpm run start",
    "build": "vite build",
    "build:test": "cross-env VITE_CIMODE=1 vite build",
    "preview:watch": "pnpm run build:test -- --watch & pnpm run serve:test",
    "serve:test": "node test-server.js",
    "test": "vitest",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}' './playwright/**/*.ts'",
    "lint:fix": "eslint --fix './src/**/*.{js,jsx,ts,tsx}' './playwright/**/*.ts'",
    "tsc": "tsc",
    "test:integration": "pnpm run build:test && START_SERVER_AND_TEST_INSECURE=1 start-server-and-test 'pnpm run serve:test' https://localhost:3000/kouta 'pnpm exec playwright test'",
    "test:ci": "CI=true pnpm run test && pnpm run test:integration",
    "analyze": "vite-bundle-visualizer",
    "xstate:typegen": "xstate typegen \"src/**/*.ts?(x)\"",
    "postinstall": "pnpm run xstate:typegen"
  },
  "dependencies": {
    "@lexical/html": "^0.38.2",
    "@lexical/link": "^0.38.2",
    "@lexical/list": "^0.38.2",
    "@lexical/react": "^0.38.2",
