    "styled-flex-component": "^3.0.5",
    "styled-media-query": "^2.1.2",
    "styled-system": "^5.1.5",
    "xstate": "^4.15.1"
  },
  "scripts": {
    "start": "craco start",
    "start:integration": "cross-env REACT_APP_CYPRESS=true DISABLE_LOCAL_PROXY=true NODE_ENV=test REACT_APP_LANG=cimode npm run start",
    "start:integration:debug": "cross-env REACT_APP_CYPRESS=true NODE_ENV=test REACT_APP_LANG=cimode npm run start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "NODE_ENV=development build-storybook -c .storybook -o build/storybook --quiet",
    "format": "prettier --write './src/**/*.{js,jsx,ts,tsx}' './cypress/**/*.{js,jsx,ts,tsx}'",
    "cypress:run": "NODE_OPTIONS=\"--max-http-header-size=8000000\" cypress run -b chrome",
    "cypress:open": "NODE_OPTIONS=\"--max-http-header-size=8000000\" cypress open -b chrome",
    "test:integration": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test start:integration https://localhost:3000 'npm run cypress:run -- --headless'",
    "test:ci": "CI=true npm run test && npm run test:integration",
    "update-mocks": "node ./scripts/update-mocks.js ./cypress/mocks"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,css,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^#(.*)$": "<rootDir>$1",
      "\\.(css|less)$": "<rootDir>/src/dummy.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(react-spring)/)"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@craco/craco": "^6.0.0",
    "@cypress/webpack-preprocessor": "^5.5.0",
    "@storybook/addon-actions": "^6.1.11",
    "@storybook/addon-essentials": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/addons": "^6.1.11",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.11",
    "@testing-library/cypress": "^7.0.3",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^10.4.9",
    "@testing-library/react-hooks": "^3.7.0",
    "@types/draft-js": "^0.10.44",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.14",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-select": "^3.0.28",
    "@types/testing-library__cypress": "^5.0.8",
    "babel-loader": "^8.2.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-styled-components": "^1.12.0",
    "cross-env": "^7.0.3",
    "cypress": "^6.2.1",
    "cypress-pipe": "^1.7.0",
    "cypress-plugin-snapshots": "^1.4.4",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.5",
    "glob": "^7.1.6",
    "http-proxy-middleware": "^1.0.6",
    "husky": "^4.3.6",
    "jest-styled-components": "^6.3.4",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "react-is": "^16.13.1",
    "react-scripts": "^4.0.1",
    "react-test-renderer": "^17.0.1",
    "start-server-and-test": "^1.11.6",
    "ts-loader": "^8.0.12",
    "typescript": "^4.1.3"
  }
}
