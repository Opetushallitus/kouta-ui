{
  "name": "kouta-ui",
  "version": "0.1.0",
  "homepage": "/kouta",
  "private": true,
  "scripts": {
    "start": "craco start",
    "start:integration": "cross-env REACT_APP_CYPRESS=true DISABLE_LOCAL_PROXY=true NODE_ENV=test REACT_APP_LANG=cimode npm run start",
    "start:integration:debug": "cross-env REACT_APP_CYPRESS=true NODE_ENV=test REACT_APP_LANG=cimode npm run start",
    "build": "craco build",
    "build:test": "cross-env REACT_APP_CYPRESS=true REACT_APP_LANG=cimode craco build",
    "serve:test": "node test-server.js",
    "test": "craco test",
    "eject": "craco eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "NODE_ENV=development build-storybook -c .storybook -o build/storybook --quiet",
    "lint:fix": "eslint --fix './src/**/*.{js,jsx,ts,tsx}' './cypress/**/*.{js,jsx,ts,tsx}'",
    "cypress:run": "NODE_OPTIONS=\"--max-http-header-size=8000000\" cypress run -b chrome",
    "cypress:open": "cross-env NODE_OPTIONS=\"--max-http-header-size=8000000\" cypress open -b chrome --env cypress-plugin-snapshots='{\"serverEnabled\": true }'",
    "test:integration": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test serve:test https://localhost:3000/kouta 'npm run cypress:run'",
    "test:ci": "CI=true npm run test && npm run test:integration",
    "update-mocks": "node ./node_modules/kto-ui-common/scripts/update-mocks.js",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "@opetushallitus/virkailija-ui-components": "^0.4.7",
    "@xstate/react": "^1.5.1",
    "axios": "^0.21.1",
    "date-fns": "^2.23.0",
    "debounce-promise": "^3.1.2",
    "draft-convert": "^2.1.12",
    "draft-js": "^0.11.7",
    "history": "^4.10.1",
    "http-status-codes": "^2.1.4",
    "i18next": "^19.9.2",
    "i18next-xhr-backend": "^3.2.2",
    "immer": "^9.0.6",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.15",
    "oph-urls-js": "github:Opetushallitus/oph-urls-js",
    "polished": "^3.7.1",
    "query-string": "^6.14.1",
    "rc-tooltip": "^5.1.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-dropzone": "^11.3.4",
    "react-i18next": "^11.11.4",
    "react-query": "^3.19.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-router-navigation-prompt": "^1.8.14",
    "react-select": "^4.3.1",
    "react-simple-timefield": "^3.2.3",
    "react-sortable-hoc": "^1.11.0",
    "react-spring": "^8.0.27",
    "react-use": "^15.3.8",
    "redux": "^4.1.1",
    "redux-actions": "^2.6.5",
    "redux-create-reducer": "^2.0.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-form": "^8.3.7",
    "redux-persist": "^6.0.0",
    "striptags": "^3.2.0",
    "styled-components": "^5.3.0",
    "styled-css-grid": "^1.2.1",
    "styled-system": "^5.1.5",
    "xstate": "^4.23.1"
  },
  "devDependencies": {
    "@babel/core": "^7.13.13",
    "@craco/craco": "^6.1.1",
    "@cypress/webpack-preprocessor": "^5.11.0",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/addons": "^6.3.6",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.6",
    "@testing-library/cypress": "^8.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^13.2.1",
    "@types/draft-js": "^0.11.4",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.172",
    "@types/node": "^14.14.37",
    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.12",
    "@types/react-redux": "^7.1.18",
    "@types/react-select": "^4.0.18",
    "@types/redux-form": "^8.3.2",
    "@types/styled-components": "^5.1.12",
    "@types/styled-system": "^5.1.12",
    "@types/testing-library__cypress": "^5.0.9",
    "@types/testing-library__jest-dom": "^5.14.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-styled-components": "^1.12.0",
    "cross-env": "^7.0.3",
    "cypress": "^9.2.1",
    "cypress-pipe": "^2.0.0",
    "cypress-plugin-snapshots": "^1.4.4",
    "cypress-terminal-report": "^3.4.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-lodash": "^7.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "glob": "^7.1.7",
    "http-proxy-middleware": "^2.0.1",
    "husky": "^4.3.8",
    "jest-styled-components": "^7.0.5",
    "kto-ui-common": "github:opetushallitus/kto-ui-common#ee04b5aacac1a8a56a1bb56091f3819d5d7b95fb",
    "lint-staged": "^10.5.4",
    "pem": "^1.14.4",
    "prettier": "^2.3.2",
    "react-scripts": "^4.0.3",
    "react-test-renderer": "^16.14.0",
    "source-map-explorer": "^2.5.2",
    "start-server-and-test": "^1.13.1",
    "ts-loader": "^8.3.0",
    "typescript": "^4.2.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,css,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^#(.*)$": "<rootDir>$1",
      "\\.(css|less)$": "<rootDir>/src/dummy.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(react-spring)/)"
    ]
  }
}
