    "@xstate/immer": "^0.3.3",
    "@xstate/inspect": "^0.8.0",
    "@xstate/react": "^3.2.2",
    "axios": "^1.11.0",
    "date-fns": "^2.30.0",
    "debounce-promise": "^3.1.2",
    "http-status-codes": "^2.3.0",
    "i18next": "^23.4.2",
    "i18next-http-backend": "^3.0.2",
    "immer": "^10.1.1",
    "lexical": "^0.38.2",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.17",
    "oph-urls-js": "github:Opetushallitus/oph-urls-js",
    "polished": "^4.3.1",
    "pretty-bytes": "^6.1.1",
    "query-string": "^9.2.1",
    "rc-tooltip": "^5.1.1",
