{
  "name": "kouta-ui",
  "version": "0.1.0",
  "homepage": "/kouta",
  "private": true,
  "scripts": {
    "prepare": "cd ../../../ && husky install",
    "start": "craco start",
    "start:integration": "cross-env DISABLE_LOCAL_PROXY=true NODE_ENV=test npm run start",
    "start:integration:debug": "cross-env REACT_APP_CIMODE=1 NODE_ENV=test npm run start",
    "build": "craco build",
    "build:test": "cross-env REACT_APP_CIMODE=1 craco build",
    "serve:test": "node test-server.js",
    "test": "craco test",
    "eject": "craco eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "NODE_ENV=development build-storybook -c .storybook -o build/storybook --quiet",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}' './playwright/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint --fix './src/**/*.{js,jsx,ts,tsx}' './playwright/**/*.{js,jsx,ts,tsx}'",
    "tsc": "tsc",
    "test:integration": "START_SERVER_AND_TEST_INSECURE=1 start-server-and-test serve:test https://localhost:3000/kouta 'npx playwright test'",
    "test:ci": "CI=true npm run test && npm run test:integration",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "xstate:typegen": "xstate typegen \"src/**/*.ts?(x)\"",
    "postinstall": "npm run xstate:typegen"
  },
  "dependencies": {
    "@lexical/link": "^0.10.0",
    "@lexical/list": "^0.10.0",
    "@lexical/react": "^0.10.0",
    "@opetushallitus/virkailija-ui-components": "^0.4.7",
    "@reduxjs/toolkit": "^1.8.0",
    "@xstate/immer": "^0.3.1",
    "@xstate/inspect": "^0.7.1",
    "@xstate/react": "^3.1.2",
    "axios": "^0.26.1",
    "date-fns": "^2.28.0",
    "debounce-promise": "^3.1.2",
    "history": "^4.10.1",
    "http-status-codes": "^2.2.0",
    "i18next": "^21.6.14",
    "i18next-http-backend": "^1.3.2",
    "immer": "^9.0.12",
    "lexical": "^0.10.0",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.15",
    "oph-urls-js": "github:Opetushallitus/oph-urls-js",
    "polished": "^4.1.4",
    "query-string": "^7.1.1",
    "rc-tooltip": "^5.1.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-dropzone": "^11.3.4",
    "react-i18next": "^11.15.6",
    "react-query": "^3.34.16",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.2.0",
    "react-router-navigation-prompt": "^1.9.6",
    "react-select": "^4.3.1",
    "react-simple-timefield": "^3.2.5",
    "react-sortable-hoc": "^1.11.0",
    "react-spring": "^8.0.27",
    "react-use": "^17.3.2",
    "redux": "^4.1.2",
    "redux-form": "^8.3.8",
    "redux-persist": "^6.0.0",
    "striptags": "^3.2.0",
    "styled-components": "^5.3.3",
    "styled-css-grid": "^1.2.1",
    "styled-system": "^5.1.5",
    "ts-pattern": "^4.1.4",
    "xstate": "^4.35.4"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@craco/craco": "^6.4.3",
    "@playwright/test": "^1.35.1",
    "@popperjs/core": "^2.11.5",
    "@storybook/addon-actions": "^6.5.4",
    "@storybook/addon-essentials": "^6.5.4",
    "@storybook/addon-links": "^6.5.4",
    "@storybook/addons": "^6.5.4",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.5.4",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/draft-js": "^0.11.9",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.179",
    "@types/node": "^16.18.38",
    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.12",
    "@types/react-redux": "^7.1.23",
    "@types/react-select": "^4.0.18",
    "@types/redux-form": "^8.3.3",
    "@types/styled-components": "^5.1.24",
    "@types/styled-system": "^5.1.15",
    "@types/testing-library__jest-dom": "^5.14.3",
    "@xstate/cli": "^0.4.2",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-styled-components": "^1.12.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-playwright": "^0.15.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "glob": "^7.2.0",
    "http-proxy-middleware": "^2.0.3",
    "https-pem": "^3.0.0",
    "husky": "^8.0.1",
    "jest-styled-components": "^7.0.8",
    "lint-staged": "^13.0.3",
    "prettier": "^2.5.1",
    "react-scripts": "^4.0.3",
    "react-test-renderer": "^16.14.0",
    "source-map-explorer": "^2.5.2",
    "start-server-and-test": "^1.14.0",
    "storybook-preset-craco": "0.0.6",
    "typescript": "^4.9.5"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{json,css,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --max-warnings 0"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^#(.*)$": "<rootDir>$1",
      "\\.(css|less)$": "<rootDir>/src/dummy.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(react-spring)/)"
    ]
  }
}
