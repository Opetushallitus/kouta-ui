{
  "name": "kouta-ui",
  "version": "0.1.0",
  "homepage": "/kouta",
  "private": true,
  "scripts": {
    "prepare": "cd ../../../ && husky install",
    "start": "vite dev",
    "start:integration": "cross-env DISABLE_LOCAL_PROXY=true NODE_ENV=test npm run start",
    "start:integration:debug": "cross-env VITE_CIMODE=1 NODE_ENV=test npm run start",
    "build": "vite build",
    "build:test": "cross-env VITE_CIMODE=1 vite build",
    "preview:watch": "npm run build:test -- --watch & npm run serve:test",
    "serve:test": "node test-server.js",
    "test": "vitest",
    "storybook": "cross-env STORYBOOK=1 sb dev -p 9009",
    "build-storybook": "cross-env STORYBOOK=1 sb build -c .storybook -o build/storybook --quiet",
    "lint": "eslint './src/**/*.{js,jsx,ts,tsx}' './playwright/**/*.ts'",
    "lint:fix": "eslint --fix './src/**/*.{js,jsx,ts,tsx}' './playwright/**/*.ts'",
    "tsc": "tsc",
    "test:integration": "npm run build:test && START_SERVER_AND_TEST_INSECURE=1 start-server-and-test serve:test https://localhost:3000/kouta 'npx playwright test'",
    "test:ci": "CI=true npm run test && npm run test:integration",
    "analyze": "vite-bundle-visualizer",
    "xstate:typegen": "xstate typegen \"src/**/*.ts?(x)\"",
    "postinstall": "npm run xstate:typegen"
  },
  "dependencies": {
    "@lexical/link": "^0.10.0",
    "@lexical/list": "^0.10.0",
    "@lexical/react": "^0.10.0",
    "@opetushallitus/virkailija-ui-components": "^0.4.7",
    "@react-spring/web": "^9.7.3",
    "@reduxjs/toolkit": "^1.9.5",
    "@xstate/immer": "^0.3.3",
    "@xstate/inspect": "^0.8.0",
    "@xstate/react": "^3.2.2",
    "axios": "^1.7.9",
    "date-fns": "^2.30.0",
    "debounce-promise": "^3.1.2",
    "history": "^4.10.1",
    "http-status-codes": "^2.2.0",
    "i18next": "^23.4.2",
    "i18next-http-backend": "^2.2.1",
    "immer": "^10.0.2",
    "lexical": "^0.10.0",
    "lodash": "^4.17.21",
    "memoizee": "^0.4.15",
    "oph-urls-js": "github:Opetushallitus/oph-urls-js",
    "polished": "^4.2.2",
    "pretty-bytes": "^6.1.1",
    "query-string": "^8.1.0",
    "rc-tooltip": "^5.1.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-dropzone": "^14.2.3",
    "react-i18next": "^13.0.3",
    "react-query": "^3.39.3",
    "react-redux": "^8.1.2",
    "react-router-dom": "^5.2.0",
    "react-router-navigation-prompt": "^1.9.6",
    "react-select": "^4.3.1",
    "react-simple-timefield": "^3.3.1",
    "react-sortable-hoc": "^1.11.0",
    "react-use": "^17.4.0",
    "redux": "^4.2.1",
    "redux-form": "^8.3.10",
    "redux-persist": "^6.0.0",
    "striptags": "^3.2.0",
    "styled-components": "^5.3.3",
    "styled-css-grid": "^1.2.1",
    "styled-system": "^5.1.5",
    "ts-pattern": "^4.1.4",
    "xstate": "^4.38.2"
  },
  "devDependencies": {
    "@optimize-lodash/rollup-plugin": "^4.0.3",
    "@playwright/test": "^1.36.2",
    "@popperjs/core": "^2.11.8",
    "@storybook/addon-actions": "^7.2.1",
    "@storybook/addon-essentials": "^7.2.1",
    "@storybook/addon-links": "^7.2.1",
    "@storybook/addons": "^7.2.1",
    "@storybook/builder-vite": "^7.2.1",
    "@storybook/cli": "^7.2.1",
    "@storybook/react-vite": "^7.2.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^12.1.4",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/lodash": "^4.14.179",
    "@types/node": "^16.18.38",
    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.12",
    "@types/react-redux": "^7.1.25",
    "@types/react-select": "^4.0.18",
    "@types/redux-form": "^8.3.5",
    "@types/styled-components": "^5.1.26",
    "@types/styled-system": "^5.1.16",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitejs/plugin-basic-ssl": "^1.0.1",
    "@vitejs/plugin-react": "^4.0.4",
    "@xstate/cli": "^0.5.2",
    "browserslist-to-esbuild": "^1.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-playwright": "^0.15.3",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "https-pem": "^3.0.0",
    "husky": "^8.0.3",
    "jest-styled-components": "^7.1.1",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.3",
    "prettier": "^3.5.3",
    "start-server-and-test": "^1.14.0",
    "typescript": "5.0",
    "vite": "^4.4.9",
    "vite-bundle-visualizer": "^0.10.0",
    "vite-plugin-checker": "^0.6.1",
    "vite-plugin-rewrite-all": "^1.0.1",
    "vite-plugin-svgr": "^3.2.0",
    "vitest": "^0.34.1"
  },
  "overrides": {
    "path-to-regexp": "1.9.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{json,css,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  }
}
