  "scripts": {
    "start": "craco start",
    "start:integration": "DISABLE_LOCAL_PROXY=true NODE_ENV=test REACT_APP_LANG=cimode npm run start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "NODE_ENV=development build-storybook -c .storybook -o build/storybook --quiet",
    "format": "prettier --write './src/**/*.{js,jsx,ts,tsx}' './cypress/**/*.{js,jsx,ts,tsx}'",
    "cypress:run": "cypress run -b chrome",
    "cypress:open": "cypress open -b chrome",
    "test:integration": "START_SERVER_AND_TEST_INSECURE=1 CYPRESS_RETRIES=2 REACT_APP_CYPRESS=true start-server-and-test start:integration https://localhost:3000 'npm run cypress:run -- --headless'",
    "test:ci": "CI=true npm run test:integration"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "./node_modules/.bin/prettier --write",
      "git add"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^#(.*)$": "<rootDir>$1",
      "\\.(css|less)$": "<rootDir>/src/dummy.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(react-spring)/)"
    ]
  },
  "devDependencies": {
