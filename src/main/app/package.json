    "vitest": "^3.2.4"
  },
  "overrides": {
    "react-sortable-hoc": {
      "react": "^18.0",
      "react-dom": "^18.0"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie 11",
    "not op_mini all"
  ],
  "lint-staged": {
    "*.{json,css,md}": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "devEngines": {
    "packageManager": {
      "name": "pnpm"
    }
  },
  "packageManager": "pnpm@10.22.0"
}
